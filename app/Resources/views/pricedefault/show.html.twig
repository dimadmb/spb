{% extends 'baseadmin.html.twig' %}

{% block body %}
    <h1>Pricedefault</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ priceDefault.id }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ priceDefault.price }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pricedefault_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pricedefault_edit', { 'id': priceDefault.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
